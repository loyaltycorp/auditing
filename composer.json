{
  "name": "loyaltycorp/auditing",
  "description": "Auditing library that adds request and response logging to applications",
  "type": "library",
  "license": "BSD-3-Clause",
  "minimum-stability": "stable",
  "require": {
    "aws/aws-sdk-php": "^3.93",
    "guzzlehttp/guzzle": "^6.3",
    "php": ">=7.1"
  },
  "require-dev": {
    "eoneopay/standards": "dev-master",
    "illuminate/console": "^5.5",
    "phpstan/phpstan": "^0.11.6",
    "phpstan/phpstan-phpunit": "^0.11.1",
    "phpstan/phpstan-strict-rules": "^0.11.0",
    "phpunit/phpunit": "^7.0",
    "roave/security-advisories": "dev-master",
    "sebastian/phpcpd": "^4.1",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "LoyaltyCorp\\Auditing\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\LoyaltyCorp\\Auditing\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
